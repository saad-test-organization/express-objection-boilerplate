{
    "env": {
      "node": true,
      "jest": true
    },
    "extends": ["plugin:jest/recommended"],
    "plugins": ["jest"],
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "no-console": "error",
      "func-names": "off",
      "no-underscore-dangle": "off",
      "consistent-return": "off",
      "jest/expect-expect": "off"
    }
  }